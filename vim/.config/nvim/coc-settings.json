{
  "metals.serverVersion": "0.9.5-SNAPSHOT",
  //"metals.serverVersion": "0.9.3+115-08650df6-SNAPSHOT",
  //"metals.serverVersion": "0.9.4",
  "metals.excludedPackages": [
    "akka.actor.typed.javadsl",
    "com.github.swagger.akka.javadsl"
  ],

  "coc.preferences.formatOnType": true,
  "codeLens.enable": true,
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "",
  "metals.statusBarEnabled": true,
  "diagnostic.checkCurrentLine": true,
  "metals.superMethodLensesEnabled": true,
  "languageserver": {
    "lua": {
      "args": [
        "-E",
        "-e",
        "LANG=en",
        "/Users/ckipp/Documents/lua-workspace/lua-language-server/main.lua"
      ],
      "command": "/Users/ckipp/Documents/lua-workspace/lua-language-server/bin/macOS/lua-language-server",
      "filetypes": [
        "lua"
      ],
      "rootPatterns": [
        ".git/"
      ]
    }
  },
  "Lua.diagnostics": {
    "disable": [
      "lowercase-global"
    ],
    "globals": [
      "vim",
      "it",
      "describe"
    ]
  },
  "coc.preferences.extensionUpdateCheck": "daily",
  "tsserver.log": "verbose",
  "tsserver.trace.server": "verbose",

}
