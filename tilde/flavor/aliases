alias vnative="nvim --cmd 'let vnative=1'"
alias t='touch'
alias 'cd..'='cd_up'
alias new-sbt='sbt new scala/scala-seed.g8'
alias mlog='tail -f .metals/metals.log'
alias metac="coursier launch org.scalameta:metac_2.12.10:4.3.1 -- -cp (coursier fetch -p org.scala-lang:scala-library:2.12.8)"
